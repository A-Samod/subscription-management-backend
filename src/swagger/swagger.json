{
  "swagger": "2.0",
  "info": {
    "title": "Subscription Management API",
    "version": "1.0.0",
    "description": "API for managing Spotify and YouTube subscriptions and user payments"
  },
  "host": "localhost:3000",
  "basePath": "/api",
  "tags": [
    {
      "name": "auth",
      "description": "User authentication"
    },
    {
      "name": "payments",
      "description": "User payments"
    },
    {
      "name": "subscriptions",
      "description": "Subscription management"
    }
  ],
  "schemes": ["http"],
  "paths": {
    "/subscriptions": {
      "get": {
        "tags": ["subscriptions"],
        "summary": "Get all subscriptions",
        "security": [
          {
            "Bearer": []
          }
        ],
        "responses": {
          "200": {
            "description": "List of subscriptions",
            "schema": {
              "type": "array",
              "items": {
                "$ref": "#/definitions/Subscription"
              }
            }
          },
          "401": {
            "description": "Unauthorized"
          }
        }
      },
      "post": {
        "tags": ["subscriptions"],
        "summary": "Create a new subscription",
        "security": [
          {
            "Bearer": []
          }
        ],
        "parameters": [
          {
            "in": "body",
            "name": "subscription",
            "description": "Subscription object",
            "schema": {
              "$ref": "#/definitions/Subscription"
            }
          }
        ],
        "responses": {
          "201": {
            "description": "Subscription created"
          },
          "401": {
            "description": "Unauthorized"
          }
        }
      }
    },
    "/subscriptions/{id}": {
      "get": {
        "tags": ["subscriptions"],
        "summary": "Get a subscription by ID",
        "security": [
          {
            "Bearer": []
          }
        ],
        "parameters": [
          {
            "name": "id",
            "in": "path",
            "required": true,
            "type": "string"
          }
        ],
        "responses": {
          "200": {
            "description": "Subscription object",
            "schema": {
              "$ref": "#/definitions/Subscription"
            }
          },
          "401": {
            "description": "Unauthorized"
          },
          "404": {
            "description": "Subscription not found"
          }
        }
      },
      "put": {
        "tags": ["subscriptions"],
        "summary": "Update a subscription by ID",
        "security": [
          {
            "Bearer": []
          }
        ],
        "parameters": [
          {
            "name": "id",
            "in": "path",
            "required": true,
            "type": "string"
          },
          {
            "in": "body",
            "name": "subscription",
            "description": "Updated subscription object",
            "schema": {
              "$ref": "#/definitions/Subscription"
            }
          }
        ],
        "responses": {
          "200": {
            "description": "Subscription updated"
          },
          "401": {
            "description": "Unauthorized"
          },
          "404": {
            "description": "Subscription not found"
          }
        }
      },
      "delete": {
        "tags": ["subscriptions"],
        "summary": "Delete a subscription by ID",
        "security": [
          {
            "Bearer": []
          }
        ],
        "parameters": [
          {
            "name": "id",
            "in": "path",
            "required": true,
            "type": "string"
          }
        ],
        "responses": {
          "200": {
            "description": "Subscription deleted"
          },
          "401": {
            "description": "Unauthorized"
          },
          "404": {
            "description": "Subscription not found"
          }
        }
      }
    },
    "/auth/register": {
      "post": {
        "tags": ["auth"],
        "summary": "Register a new user",
        "parameters": [
          {
            "in": "body",
            "name": "user",
            "description": "User object",
            "schema": {
              "$ref": "#/definitions/User"
            }
          }
        ],
        "responses": {
          "201": {
            "description": "User registered"
          },
          "400": {
            "description": "Bad request"
          }
        }
      }
    },
    "/auth/login": {
      "post": {
        "tags": ["auth"],
        "summary": "Log in a user",
        "parameters": [
          {
            "in": "body",
            "name": "credentials",
            "description": "User credentials",
            "schema": {
              "type": "object",
              "properties": {
                "username": {
                  "type": "string"
                },
                "password": {
                  "type": "string"
                }
              }
            }
          }
        ],
        "responses": {
          "200": {
            "description": "User logged in",
            "schema": {
              "type": "object",
              "properties": {
                "token": {
                  "type": "string"
                }
              }
            }
          },
          "400": {
            "description": "Invalid credentials"
          },
          "404": {
            "description": "User not found"
          }
        }
      }
    },
    "/payments": {
      "post": {
        "tags": ["payments"],
        "summary": "Add a payment",
        "security": [
          {
            "Bearer": []
          }
        ],
        "parameters": [
          {
            "in": "body",
            "name": "payment",
            "description": "Payment object",
            "schema": {
              "$ref": "#/definitions/Payment"
            }
          }
        ],
        "responses": {
          "201": {
            "description": "Payment added"
          },
          "400": {
            "description": "Bad request"
          },
          "401": {
            "description": "Unauthorized"
          }
        }
      },
      "get": {
        "tags": ["payments"],
        "summary": "Get user payments",
        "security": [
          {
            "Bearer": []
          }
        ],
        "responses": {
          "200": {
            "description": "List of user payments",
            "schema": {
              "type": "array",
              "items": {
                "$ref": "#/definitions/Payment"
              }
            }
          },
          "401": {
            "description": "Unauthorized"
          }
        }
      }
    }
  },
  "definitions": {
    "User": {
      "type": "object",
      "required": ["username", "password", "role"],
      "properties": {
        "username": {
          "type": "string"
        },
        "password": {
          "type": "string"
        },
        "role": {
          "type": "string",
          "enum": ["admin", "member"]
        }
      }
    },
    "Payment": {
      "type": "object",
      "required": ["userId", "userName", "platform", "month", "amount", "paymentDate"],
      "properties": {
        "userId": {
          "type": "string"
        },
        "userName": {
          "type": "string"
        },
        "platform": {
          "type": "array",
          "items": {
            "type": "string",
            "enum": ["spotify", "youtube"]
          }
        },
        "month": {
          "type": "string"
        },
        "amount": {
          "type": "number"
        },
        "paymentDate": {
          "type": "string",
          "format": "date"
        },
        "remarks": {
          "type": "string"
        },
        "attachments": {
          "type": "string"
        },
        "createdAt": {
          "type": "string",
          "format": "date-time"
        },
        "updatedAt": {
          "type": "string",
          "format": "date-time"
        }
      }
    },
    "Subscription": {
      "type": "object",
      "required": ["userId", "platform", "startDate", "endDate"],
      "properties": {
        "userId": {
          "type": "string"
        },
        "platform": {
          "type": "string",
          "enum": ["spotify", "youtube"]
        },
        "startDate": {
          "type": "string",
          "format": "date"
        },
        "endDate": {
          "type": "string",
          "format": "date"
        },
        "isActive": {
          "type": "boolean"
        }
      }
    }
  },
  "securityDefinitions": {
    "Bearer": {
      "type": "apiKey",
      "name": "auth-token",
      "in": "header"
    }
  }
}
